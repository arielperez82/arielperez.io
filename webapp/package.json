{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "22.11.0",
    "pnpm": "10.8.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --collectCoverage",
    "test:e2e": "playwright test -c test/e2e/config/playwright.local.ts",
    "test:e2e:ci": "playwright test -c test/e2e/config/playwright.ci.ts",
    "test:e2e:smoke": "playwright test -c test/e2e/config/playwright.smoke.ts",
    "test:e2e:debug": "playwright test -c test/e2e/config/playwright.local.ts --debug",
    "supabase:start": "supabase start --exclude studio,storage-api,supavisor,vector,postgrest,logflare,imgproxy,realtime",
    "supabase:stop": "supabase stop"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.1",
    "@heroicons/react": "^2.2.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-query": "^5.72.1",
    "next": "15.2.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/istanbul-lib-coverage": "^2.0.6",
    "@types/istanbul-lib-instrument": "^1.7.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-istanbul": "^7.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.5",
    "eslint-import-resolver-typescript": "^4.3.4",
    "istanbul-lib-coverage": "^3.2.2",
    "istanbul-lib-instrument": "^6.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nyc": "^17.1.0",
    "playwright-test-coverage": "^1.2.12",
    "supabase": "^2.20.12",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
